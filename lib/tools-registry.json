  "tools_to_remove": [
    {
      "name": "glow",
      "reason": "Low value - most docs viewed in browser or IDE",
      "command": "brew uninstall glow"
    },
    {
      "name": "pipx",
      "reason": "Replaced by uv for Python tool management",
      "command": "brew uninstall pipx"
    },
    {
      "name": "coderabbit",
      "reason": "Not actively used, overlaps with other AI tools",
      "command": "brew uninstall --cask coderabbit"
    },
    {
      "name": "@kilocode/cli",
      "reason": "Not actively used, have Claude Code and Codex",
      "command": "npx pnpm uninstall -g @kilocode/cli"
    },
    {
      "name": "netlify-cli",
      "reason": "If not using Netlify, remove (keep Vercel)",
